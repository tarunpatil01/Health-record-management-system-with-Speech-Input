<% layout("/layouts/boilerplate") %>

<div class="row mt-4">
    <h3>General Details : </h3>
    <ul>
        <li><%= general.info %></li>
    </ul> 
    <br><br>
    <div class="d-flex">
        <a href="/generals/<%= general._id %>/edit" class="btn btn-dark me-2">Edit</a>

        <form method="post" action="/generals/<%= general._id %>?_method=DELETE" class="mb-0">
            <button class="btn btn-dark">Delete</button>
        </form>
    </div>
</div>
